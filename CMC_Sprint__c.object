    <validationRules>
        <fullName>CMC_MandatoryReleaseValidation</fullName>
        <active>true</active>
        <description>Make Release a required field on Sprint</description>
        <errorConditionFormula>ISBLANK(Release__c)</errorConditionFormula>
        <errorDisplayField>Release__c</errorDisplayField>
        <errorMessage>Please select a Release for this Sprint</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CMC_SprintDateValidation</fullName>
        <active>true</active>
        <description>End Date cannot be earlier than Start Date</description>
