    <fields>
        <fullName>Days_Remaining__c</fullName>
        <description>Days remaining within the sprint schedule.</description>
        <externalId>false</externalId>
        <formula>IF(TODAY() &gt; End_Date__c, 
    0, 
    IF(TODAY() &lt; Start_Date__c, 
        (End_Date__c - Start_Date__c), 
        (End_Date__c - TODAY()) 
    ) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Days remaining within the sprint schedule.</inlineHelpText>
        <label>Days Remaining</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Description__c</fullName>
        <description>Description of the sprint.</description>
        <externalId>false</externalId>
        <inlineHelpText>Description of the sprint.</inlineHelpText>
        <label>Description</label>
        <length>32000</length>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>true</trackHistory>
        <type>LongTextArea</type>
        <visibleLines>3</visibleLines>
    </fields>
