// 
// Check first to see if this is a portal user. If so, display a graceful error message, since portal users don&apos;t 
// have access to Chatter-related objects. 
// 
var isPortalUser = (sforce.connection.getUserInfo().userType != &quot;Standard&quot;); 
if (isPortalUser) { 
alert(&quot;Unfortunately, SFDC portal users don&apos;t have access to Chatter, so the Follow button isn&apos;t available for use.&quot;); 
} else { 

if (records[0] == null) { 
alert(&quot;Please select at least one record.&quot;); 
} 
else { 
var follow_count = 0; 

LBox(); 

